/* pages/mine/travels/travels.wxss */

.co-write {
  position: fixed;
  bottom: 50rpx;
  width: 100%;
  z-index: 99;
}

.write-img {
  width: 50rpx;
  height: 50rpx;
}

.mask-travel {
  width: 100%;
  height: 100%;
  position: fixed;
  background-color: black;
  z-index: 99;
  top: 0;
  left: 0;
  opacity: 0.7;
}
.mask{
  display: flex;
  flex-direction: column;
  position: fixed;
  z-index: 150;
}